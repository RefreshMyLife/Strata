{
  "name": "strata-ui-tranches",
  "version": "2.15.3",
  "private": true,
  "engines": {
    "node": ">=18.x.x"
  },
  "scripts": {
    "start": "vite --host",
    "build": "node ./version.js && vite build",
    "serve": "vite preview",
    "test": "TZ=UTC VITE_ENVIRONMENT=testnet vitest run",
    "test:ci": "TZ=UTC vitest run",
    "lint:format": "yarn prettier \"src/**/*.{js,jsx,ts,tsx,html,json,md}\"",
    "lint": "npm-run-all --parallel lint:ts lint:styles lint:format",
    "lint:ts": "export NODE_OPTIONS=\"--max-old-space-size=4096\" && eslint . --cache",
    "lint:styles": "stylelint 'src/**/*.{css,scss,ts,tsx,js,jsx}'",
    "typecheck": "tsc",
    "format": "yarn lint:format --write && yarn lint:ts --fix && yarn lint:styles --fix",
    "extract-translation-keys": "i18next",
    "generate-contract-types": "rimraf ./src/packages/types/contracts && typechain --target=ethers-v5 --out-dir=./src/packages/contracts/types/contracts '../strata-contracts/artifacts/contracts/**/*[!.dbg].json' && typechain --target=ethers-v5 --out-dir=./src/packages/contracts/types/external './src/packages/contracts/contractInfos/externalAbis/*.json'",
    "generate-version-file": "genversion --es6 --semi src/constants/version.ts",
    "husky:install": "husky install",
    "//postinstall": "npm-run-all --parallel husky:install generate-version-file",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "regression": "reg-suit run",
    "publish-test": "npm run build && publish.bat && cd out-git && git add -A && git commit -m publish && git push origin HEAD:stage-test",
    "publish": "npm run build && publish.bat && cd out-git && git add -A && git commit -m publish && git push origin HEAD:stage-release"
  },
